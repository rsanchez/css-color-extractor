{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": [2, "always-multiline"],
        "space-before-function-paren": [2, "never"],
        "semi": [2, "always"]
    }
}
